{
  "TS38331_snippet.txt": [
    {
      "text": "The UE shall perform the following actions upon reception of the RRCSetup:",
      "logic": [
        {
          "type": "MSG_RECEIVE",
          "data": {
            "msg": "RRCSetup"
          }
        }
      ]
    },
    {
      "text": "1> stop timer T300, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T300"
          }
        }
      ]
    },
    {
      "text": "2> stop timer T319, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T319"
          }
        }
      ]
    },
    {
      "text": "2> stop timer T302, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T302"
          }
        }
      ]
    },
    {
      "text": "2> stop timer T304, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T304"
          }
        }
      ]
    },
    {
      "text": "3> set the registeredAMF to the value received from upper layers, if available;",
      "logic": [
        {
          "type": "VAR_SET",
          "data": {
            "var": "registeredAMF",
            "val": "the value received from upper layers, if available;"
          }
        }
      ]
    },
    {
      "text": "1> stop timer T320, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T320"
          }
        }
      ]
    },
    {
      "text": "1> stop timer T325, if running;",
      "logic": [
        {
          "type": "TIMER_STOP",
          "data": {
            "timer": "T325"
          }
        }
      ]
    }
  ],
  "TS38321_snippet.txt": [
    {
      "text": "The RRC entity triggers the Random Access procedure by providing to the MAC entity:",
      "logic": [
        {
          "type": "INTER_LAYER_TRIGGER",
          "data": {
            "proc": "Random Access"
          }
        }
      ]
    },
    {
      "text": "1> set the PREAMBLE_TRANSMISSION_COUNTER to 1;",
      "logic": [
        {
          "type": "VAR_SET",
          "data": {
            "var": "PREAMBLE_TRANSMISSION_COUNTER",
            "val": "1;"
          }
        }
      ]
    },
    {
      "text": "1> set the PREAMBLE_POWER_RAMPING_COUNTER to 1;",
      "logic": [
        {
          "type": "VAR_SET",
          "data": {
            "var": "PREAMBLE_POWER_RAMPING_COUNTER",
            "val": "1;"
          }
        }
      ]
    },
    {
      "text": "1> instruct the physical layer to transmit a preamble using the selected PRACH;",
      "logic": [
        {
          "type": "CROSS_LAYER_ACT",
          "data": {
            "entity": "physical layer",
            "act": "transmit a preamble using the selected PRACH;"
          }
        }
      ]
    },
    {
      "text": "1> set the PREAMBLE_TRANSMISSION_COUNTER to PREAMBLE_TRANSMISSION_COUNTER + 1;",
      "logic": [
        {
          "type": "VAR_SET",
          "data": {
            "var": "PREAMBLE_TRANSMISSION_COUNTER",
            "val": "PREAMBLE_TRANSMISSION_COUNTER + 1;"
          }
        }
      ]
    },
    {
      "text": "If the Random Access Response is successfully received:",
      "logic": [
        {
          "type": "MSG_SUCCESS",
          "data": {
            "msg": "Random Access Response"
          }
        }
      ]
    },
    {
      "text": "1> indicate the reception of the Random Access Response to upper layers.",
      "logic": [
        {
          "type": "INTER_LAYER_IND",
          "data": {
            "evt": "reception of the Random Access Response",
            "target": "upper layers"
          }
        }
      ]
    }
  ]
}